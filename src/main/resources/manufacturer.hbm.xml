<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="springhibernateconfiguration.Manufacturer" table="manufacturer_SPring">

		<id name="manufacturer_id" type="java.lang.Integer">
			<!-- <column name="osid" unique="true" /> <column name="id" /> -->
			<!-- <generator class="identity" /> -->
		</id>

		<property name="country" type="string">
			<column name="country" length="20" not-null="true" />
		</property>
		<property name="manufacturername" type="string">
			<column name="manufacturername" length="20" not-null="true" />
		</property>
	</class>
</hibernate-mapping>